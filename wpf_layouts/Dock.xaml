<Window x:Class="wpf_layouts.Dock"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpf_layouts"
        mc:Ignorable="d"
        Title="Dock" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Top">Top1</Button>
            <Button DockPanel.Dock="Bottom">Bottom1</Button>
            <Button DockPanel.Dock="Bottom">Bottom2</Button>
            <Button DockPanel.Dock="Left">Left1</Button>
            <Button DockPanel.Dock="Right">Right1</Button>
            <Button DockPanel.Dock="Top">Top2</Button>
            <Button DockPanel.Dock="Right">Right2</Button>
            <Button>Center</Button>
        </DockPanel>

    </Grid>
</Window>
